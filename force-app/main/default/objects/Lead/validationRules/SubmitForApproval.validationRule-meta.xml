<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>SubmitForApproval</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
  ISCHANGED(Status),
  ISPICKVAL(Status, &apos;Qualified&apos;),
  NOT(SubmitforApproval__c)
)</errorConditionFormula>
    <errorMessage>Can not Convert Lead. You Have Submit first For Approval</errorMessage>
</ValidationRule>
